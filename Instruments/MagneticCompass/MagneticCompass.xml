<?xml version="1.0"?>

<PropertyList>

 <path>MagneticCompass.ac</path>

 <animation>
  <type>material</type>
  <object-name>Ball</object-name>
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.0</blue>
   <factor-prop>sim/model//material/instruments/factor</factor-prop>
  </emission>
 </animation>


 <animation>
  <type>rotate</type>
  <object-name>Ball</object-name>

  <property>instrumentation/magnetic-compass/indicated-heading-deg</property>
  <center>
   <x-m>-0.022</x-m>
   <y-m>0.00</y-m>
   <z-m>0</z-m>
  </center> 
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

</PropertyList>
