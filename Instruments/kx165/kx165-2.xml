      <binding>
        <command>property-assign</command>
        <property alias="../../../../params/power"/>
        <value>1</value>
        <condition>
          <greater-than>
            <property alias="../../../../../../params/comm-volume"/>
            <value>0.05</value>
          </greater-than>
        </condition>
      </binding>
    </action>
  </animation>

