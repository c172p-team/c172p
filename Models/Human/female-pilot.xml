<!-- hip -->
	<animation>
		<type>rotate</type>
		<property>sim/model/crew/pilot/pose/position/limb[0]/y-deg</property>
		<offset-deg>0</offset-deg>
		<center>
			<x-m>0.01</x-m>
			<y-m>0.0</y-m>
			<z-m>0.1</z-m>
		</center>
		<axis>
			<x>0.0</x>
			<y>1.0</y>
			<z>0.0</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<property>sim/model/crew/pilot/pose/position/limb[0]/z-m</property>
