<?xml version="1.0"?>

<PropertyList>
    <name>GPSModel-fg1000</name>
    <path>gpsmodel-fg1000.ac</path>

    <effect>
        <inherits-from>Aircraft/c172p/Models/Effects/interior/lm-fg1000-gpsmodel</inherits-from>
        <object-name>gpsmodel-fg1000</object-name>
    </effect>

    <animation>
        <type>material</type>
        <object-name>gpsmodel-fg1000</object-name>
        <condition>
            <or>
				<not>
					<property>/sim/rendering/shaders/skydome</property>
				</not>
				<equals>
					<property>/sim/rendering/shaders/model</property>
					<value>0</value>
				</equals>
			</or>
        </condition>
        <emission>
            <red-prop>/sim/model/c172p/lighting/rgb-ra-r-factor</red-prop>
            <green-prop>/sim/model/c172p/lighting/rgb-ra-g-factor</green-prop>
            <blue-prop>/sim/model/c172p/lighting/rgb-ra-b-factor</blue-prop>
        </emission>
    </animation>

</PropertyList>
