    <animation>
        <type>translate</type>
        <object-name>COMchan</object-name>
        <factor>-0.001</factor>
        <property>instrumentation/comm[0]/channel-button</property>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    
    <animation>
        <type>knob</type>
        <object-name>COMknobKHZ</object-name>
        <property alias="/params/comm-dial-khz"/>
        <factor>-0.50</factor>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>-0.34726</x-m>
            <y-m> 0.09337</y-m>
            <z-m> 0.07401</z-m>
        </center>
        <action>
            <binding>
                <command>property-adjust</command>
                <property alias="/params/comm-dial-khz"/>
                <factor>4</factor>
                <min>0</min>
                <max>1440</max>
                <wrap>true</wrap>
            </binding>
        </action>
        <increase>
            <binding>
                <command>nasal</command>
                <script>
                    c172p.increment(0, 1);
                </script>
            </binding>
            <binding>
                <command>nasal</command>
                <script>c172p.click("kx165-1-dial")</script>
            </binding>
        </increase>
        <decrease>
            <binding>
                <command>nasal</command>
                <script>
                    c172p.increment(0, -1);
                </script>
            </binding>
            <binding>
                <command>nasal</command>
                <script>c172p.click("kx165-1-dial")</script>
            </binding>
        </decrease>
        <shift-action>
            <repeatable>0</repeatable>
            <binding>
                <command>property-cycle</command>
                <property>instrumentation/comm[0]/channel-mode-selector</property>
                <value>0</value>
                <value>1</value>
            </binding>
        </shift-action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>comm1-kHz</tooltip-id>
                <label>Adjust COM1 kHz; Shift click to toggle 8.33</label>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>COMknobMHZ</object-name>
        <property alias="/params/comm-dial-mhz"/>
        <factor>-0.50</factor>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>-0.34954</x-m>
            <y-m> 0.09337</y-m>
            <z-m> 0.07401</z-m>
        </center>
        <action>
            <binding>
                <condition>
                    <property alias="../../../../../params/power-comm"/>
                </condition>
                <command>property-adjust</command>
                <property alias="/params/comm-frq-standby"/>
                <factor>1</factor>
                <min>118.0</min>
                <max>136.99167</max>
                <wrap>true</wrap>
                <mask>integer</mask>
            </binding>
            <binding>
                <command>property-adjust</command>
                <property alias="/params/comm-dial-mhz"/>
                <factor>4</factor>
                <min>0</min>
                <max>360</max>
                <wrap>true</wrap>
            </binding>
            <binding>
                <command>nasal</command>
                <script>c172p.click("kx165-1-dial")</script>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>comm1-MHz</tooltip-id>
                <label>Adjust COM1 MHz</label>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>COMxfer</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property alias="../../../../params/comm-frq-swp"/>
                <value type="int">1</value>
            </binding>
            <binding>
                <command>property-swap</command>
                <property alias="../../../../params/comm-frq-selected"/>
                <property alias="../../../../params/comm-frq-standby"/>
            </binding>
            <binding>
                <command>nasal</command>
                <script>c172p.click("kx165-1")</script>
            </binding>
            <mod-up>
                <binding>
                    <command>property-assign</command>
                    <property alias="../../../../../params/comm-frq-swp"/>
                    <value type="int">0</value>
                </binding>
                <binding>
                    <command>nasal</command>
                    <script>c172p.click("kx165-1")</script>
                </binding>
            </mod-up>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>comm1-toggle</tooltip-id>
                <label>Swap COM1 frequencies</label>
            </binding>
        </hovered>
    </animation>

    <!-- Nav part -->
    <animation>
        <type>knob</type>
        <object-name>NAVvol</object-name>
        <property alias="../../params/nav-volume"/>
        <factor>-130</factor>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>-0.34939</x-m>
            <y-m> 0.13118</y-m>
            <z-m> 0.07122</z-m>
        </center>
        <action>
            <binding>
                <command>property-adjust</command>
                <property alias="../../../../params/nav-volume"/>
                <factor>0.1</factor>
                <min>0.0</min>
                <max>1.0</max>
                <wrap>false</wrap>
            </binding>
            <binding>
                <command>nasal</command>
                <script>c172p.click("kx165-1-dial")</script>
            </binding>
        </action>
        <shift-action>
            <binding>
                <command>property-toggle</command>
                <property alias="../../../../params/nav-audio"/>
            </binding>
        </shift-action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>nav1-volume</tooltip-id>
                <label>%s</label>
                <measure-text>NAV1 Volume: 100% ; Ident disabled (Shift + click to pull)</measure-text>
                <property alias="../../../../params/nav-volume"/>
                <mapping>nasal</mapping>
                <script>
                    var v = arg[0];
                    var a = getprop("/instrumentation/nav[0]/ident-audible");

                    var s = "NAV1 Volume: ";
                    s = s ~ sprintf("%3d", (v * 100) + .1); # rounding errors
                    s = s ~ "% ; Ident ";
                    if (a)
                        s = s ~ "enabled (Shift + click to push)";
                    else
                        s = s ~ "disabled (Shift + click to pull)";
                    return s;
                </script>
            </binding>
        </hovered>
    </animation>

