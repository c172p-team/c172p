        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <!-- Actions -->
    <!-- Comm part -->
    <animation>
        <type>translate</type>
        <object-name>COMvol</object-name>
        <property alias="../../params/comm-volume-mode"/>
        <factor>0.002</factor>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>COMvol</object-name>
        <property alias="../../params/comm-volume"/>
        <factor>-130</factor>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>-0.34939</x-m>
            <y-m> 0.05082</y-m>
            <z-m> 0.07122</z-m>
        </center>
        <action>
            <binding>
                <condition>
                    <not><property alias="../../../../../../params/comm-volume-mode"/></not>
                </condition>
                <command>property-adjust</command>
                <property alias="../../../../params/comm-volume"/>
                <factor>0.1</factor>
                <min>0.0</min>
                <max>1.0</max>
                <wrap>false</wrap>
            </binding>
            <binding>
                <command>property-assign</command>
                <property alias="../../../../params/power-comm"/>
                <value>1</value>
                <condition>
                    <and>
                        <not><property alias="../../../../../../../params/comm-volume-mode"/></not>
                        <greater-than>
                            <property alias="../../../../../../../params/comm-volume"/>
                            <value>0.05</value>
                        </greater-than>
                    </and>
                </condition>
            </binding>
            <binding>
                <command>property-assign</command>
                <property alias="../../../../params/power-comm"/>
                <value>0</value>
                <condition>
                    <and>
                        <not><property alias="../../../../../../../params/comm-volume-mode"/></not>
                        <less-than>
                            <property alias="../../../../../../../params/comm-volume"/>
                            <value>0.05</value>
                        </less-than>
                    </and>
                </condition>
            </binding>
            <binding>
                <command>property-assign</command>
                <property alias="../../../../params/power-nav"/>
                <value>1</value>
                <condition>
                    <and>
                        <not><property alias="../../../../../../../params/comm-volume-mode"/></not>
                        <greater-than>
                            <property alias="../../../../../../../params/comm-volume"/>
                            <value>0.05</value>
                        </greater-than>
                    </and>
                </condition>
            </binding>
            <binding>
                <command>property-assign</command>
                <property alias="../../../../params/power-nav"/>
                <value>0</value>
                <condition>
                    <and>
                        <not><property alias="../../../../../../../params/comm-volume-mode"/></not>
                        <less-than>
                            <property alias="../../../../../../../params/comm-volume"/>
                            <value>0.05</value>
                        </less-than>
                    </and>
                </condition>
            </binding>
            <binding>
                <condition>
                    <property alias="../../../../../params/comm-volume-mode"/>
                </condition>
                <command>property-adjust</command>
                <property alias="../../../../params/comm-squelch"/>
                <factor>0.1</factor>
                <min>0.0</min>
                <max>1.0</max>
                <wrap>false</wrap>
            </binding>
            <binding>
                <command>nasal</command>
                <script>c172p.click("kx165-1-dial")</script>
            </binding>
        </action>
        <shift-action>
            <binding>
                <condition>
                    <property alias="../../../../../params/comm-volume-mode"/>
                </condition>
                <command>property-assign</command>
                <property alias="../../../../params/comm-squelch"/>
                <value>0.4</value>
            </binding>
            <binding>
                <command>property-toggle</command>
                <property alias="../../../../params/comm-volume-mode"/>
            </binding>
            <binding>
                <command>nasal</command>
                <script>c172p.click("kx165-1-dial")</script>
            </binding>
        </shift-action>
        <hovered>
            <binding>
                <condition>
                    <not><property alias="../../../../../../params/comm-volume-mode"/></not>
                </condition>
                <command>set-tooltip</command>
                <tooltip-id>comm1-volume</tooltip-id>
                <label>COM1 Volume: %d%%</label>
                <property alias="../../../../params/comm-volume"/>
                <mapping>percent</mapping>
            </binding>
            <binding>
                <condition>
                    <property alias="../../../../../params/comm-volume-mode"/>
                </condition>
                <command>set-tooltip</command>
                <tooltip-id>comm1-squelch</tooltip-id>
                <label>COM1 Squelch: %d%%</label>
                <property alias="../../../../params/comm-squelch"/>
                <mapping>percent</mapping>
            </binding>
        </hovered>
