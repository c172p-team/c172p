{ 
    "src":    "DG.svg",

    "animations": [
      {
        "element": "#Card",
        "type":    "transform",
        "transforms": [{
          "type": "rotate",
          "a": {
            "property": "heading",
            "scale": -1,
            "precision": 5
          },
          "x": 256,
          "y": 256
        }]
      },{
        "element": "#HeadingBug",
        "type":    "transform",
        "transforms": [{
          "type": "rotate",
          "a": {
            "property": "headingBug",
            "scale": 1
          },
          "x": 256,
          "y": 256
        }]
      }
    ]
}

