<?xml version="1.0"?>

<PropertyList>

  <name type="string">cruise</name>

  <overlay>

    <sim>
      <freeze>
	      <master type="bool">0</master>
	      <clock type="bool">0</clock>
      </freeze>
      <presets>
	      <speed-set type="string">knots</speed-set>
	      <airspeed-kt type="int">100</airspeed-kt>
	      <longitude-deg type="double">-151.422</longitude-deg>
	      <latitude-deg type="double">59.627</latitude-deg>
	      <onground type="bool">0</onground>
	      <altitude-ft type="int">6510</altitude-ft>
	      <heading-deg type="double">100</heading-deg>
      </presets>
      <model>
	      <c172p>
          <securing>
		        <brake-parking type="bool">true</brake-parking>
          </securing>
	      </c172p>
      </model>
    </sim>
    <fdm>
      <jsbsim>
	      <bushkit type="int">0</bushkit>
	      <running type="bool">0</running>
        <inertia>
            <pointmass-weight-lbs n="0">190</pointmass-weight-lbs>
            <pointmass-weight-lbs n="1">0</pointmass-weight-lbs>
        </inertia>
      </jsbsim>
    </fdm>
    <consumables>
      <fuel>
	      <tank n="0">
		      <selected type="bool">true</selected>
	      </tank>
	      <tank n="1">
		      <selected type="bool">true</selected>
	      </tank>
      </fuel>
    </consumables>
    <engines>
      <active-engine>
        <running type="bool">0</running>
        <auto-start type="bool">1</auto-start>
        <cranking type="bool">1</cranking>
      </active-engine>
    </engines>
    <controls>
      <flight>
	      <flaps type="double">0</flaps>
      </flight>
      <gear>
	      <gear-down type="bool">0</gear-down>
      </gear>
      <switches>
	      <magnetos type="int">3</magnetos>
	      <master-bat type="bool">1</master-bat>
	      <master-alt type="bool">1</master-alt>
	      <master-avionics type="bool">1</master-avionics>
	      <starter type="bool">1</starter>
      </switches>
      <engines>
        <engine n="0">
          <primer type="double">3</primer>
          <primer-lever type="bool">0</primer-lever>
        </engine>
        <current-engine>
          <throttle type="double">0.75</throttle>
          <mixture type="double">0.95</mixture>
        </current-engine>
        <active-engine>
          <oil-level type="double">7.0</oil-level>
          <carb_ice type="double">0.0</carb_ice>
        </active-engine>
        <active-engine type="int">0</active-engine>
      </engines>
    </controls>

  </overlay>

</PropertyList>
