<?xml version="1.0"?>

<PropertyList>

    <name type="string">Hanger</name>

    <description>

        The aircraft will be "parked" in the Aloha Air Cargo hanger at PHNL, cold and dark.
        Partially secured inside a hanger. No additional parameters required.

        Start state with --state=Hanger

        Additional parameters required
            -none-

    </description>

    <overlay>
        <sim>
            <presets>
                <speed-set type="string">knots</speed-set>
                <airspeed-kt type="int">0</airspeed-kt>
                <longitude-deg type="double">-157.9265297</longitude-deg>
                <latitude-deg type="double">21.33236559</latitude-deg>
                <onground type="bool">true</onground>
                <altitude-ft type="int">0</altitude-ft>
                <heading-deg type="double">182</heading-deg>
            </presets>
        </sim>
    </overlay>

</PropertyList>
