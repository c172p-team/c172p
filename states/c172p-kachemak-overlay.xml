<?xml version="1.0"?>

<PropertyList>

    <name type="string">Kachemak Bay, amphibious</name>

    <description>

        The aircraft will be on "approach" over Kachemak Bay, flaps set, gear up, engines running.
        Equiped with the 180hp engine, amphibious. No additional parameters required.

        Additional parameters required
            -none-

    </description>

    <overlay>
        <sim>
            <presets>
                <speed-set type="string">knots</speed-set>
                <airspeed-kt type="int">80</airspeed-kt>
                <longitude-deg type="double">-151.422</longitude-deg>
                <latitude-deg type="double">59.627</latitude-deg>
                <altitude-ft type="int">510</altitude-ft>
                <heading-deg type="double">150</heading-deg>
                <pitch-deg type="double">0</pitch-deg>
                <roll-deg type="double">0</roll-deg>
                <uBody-fps type="double">135</uBody-fps>
                <glideslope-deg type="double">0</glideslope-deg>
                <trim type="bool">true</trim>
                <onground type="bool">false</onground>
            </presets>
        </sim>
    </overlay>

</PropertyList>
