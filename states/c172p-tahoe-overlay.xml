<?xml version="1.0"?>

<PropertyList>

    <name type="string">Lake Tahoe, floats</name>

    <description>

        The aircraft will be on "approach" to Lake Tahoe, flaps set, engines running.
        Equipped with the 160hp engine and floats. No additional parameters required.

        Additional parameters required
            -none-

    </description>

    <overlay>
        <sim>
            <presets>
                <speed-set type="string">knots</speed-set>
                <airspeed-kt type="int">80</airspeed-kt>
                <longitude-deg type="double">-120.037</longitude-deg>
                <latitude-deg type="double">38.924</latitude-deg>
                <altitude-ft type="int">7124</altitude-ft>
                <heading-deg type="double">30</heading-deg>
                <pitch-deg type="double">0</pitch-deg>
                <roll-deg type="double">0</roll-deg>
                <uBody-fps type="double">135</uBody-fps>
                <glideslope-deg type="double">0</glideslope-deg>
                <trim type="bool">true</trim>
                <onground type="bool">false</onground>
            </presets>
        </sim>
    </overlay>

</PropertyList>
