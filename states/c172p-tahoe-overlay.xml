<?xml version="1.0"?>

<PropertyList>

    <name type="string">Tahoe</name>

    <description>

        The aircraft will be on "approach" to Lake Tahoe, flaps set, engines running.
        Equiped with the 160hp engine and floats. No additional parameters required.

        Start state with --state=Tahoe

        Additional parameters required
            -none-

    </description>

    <overlay>
        <sim>
            <presets>
                <speed-set type="string">knots</speed-set>
                <airspeed-kt type="int">80</airspeed-kt>
                <longitude-deg type="double">-120.037</longitude-deg>
                <latitude-deg type="double">38.924</latitude-deg>
                <onground type="bool">0</onground>
                <altitude-ft type="int">7124</altitude-ft>
                <heading-deg type="double">30</heading-deg>
            </presets>
        </sim>
    </overlay>

</PropertyList>
